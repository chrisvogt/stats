{
  "name": "chrisvogt/stats",
  "description": "Developer stats based on WakaTime data.",
  "type": "project",
  "keywords" : ["WakaTime", "developer", "CakePHP", "reporting"],
  "homepage" : "http://stats.chrisvogt.me",
  "license" : "MIT",
  "require" : {
    "php" : ">=5.4",
    "composer/installers" : "~1.0",
    "cakephp/cakephp" : "2.5.*",
    "friendsofcake/crud" : "3.*",
    "josegonzalez/dotenv": "0.3.*",
    "ad7six/dsn": "0.3.*",
    "chrisvogt/cakephp-wakatime": "~0.9"
  },
  "require-dev": {
    "cakephp/debug_kit" : "2.2.*"
  },
  "minimum-stability": "dev",
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "heroku": {
      "framework": "cakephp2",
      "document-root": "webroot",
      "index-document": "index.php"
    }
  }
}
